{
  "editor.tabSize": 2,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.associations": {
    "*.html": "html"
  },
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,
  "keyboard.dispatch": "keyCode",
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex fh-toolchain",
      "tools": [
        "create_tmp",
        "pdflatex",
        "biber",
        "pdflatex"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "biber",
      "command": "biber",
      "args": [
        "./tmp/thesis.bcf"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-output-directory=tmp",
        "%DOC%"
      ]
    },
    {
      "name": "create_tmp",
      "command": "mkdir",
      "args": [
        "-p",
        "./tmp"
      ]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "./tmp",
  "window.titleBarStyle": "custom",
  "editor.fontFamily": "'JetBrains Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.fontFamily": "\"Source Code Pro\",\"Meslo LG M DZ for Powerline\"",
  "terminal.integrated.fontSize": 16,
  "java.errors.incompleteClasspath.severity": "ignore",
  "todo-tree.highlights.customHighlight": {
    "TODO": {},
    "FIXME": {}
  },
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "green",
    "type": "none"
  },
  "todo-tree.regex.regex": "((//|#|%+?|<!--|;|/\\*|^)\\s*($TAGS)|^\\s*- \\[ \\])",
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "todo"
  ],
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "files.hotExit": "onExit",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "python.languageServer": "Default",
  "java.project.importHint": false,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorTheme": "Default Light+",
  "prettier.singleQuote": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.config/vim/vimrc",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.fontSize": 16,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "git.openRepositoryInParentFolders": "never",
  "markdown-pdf.plantumlOpenMarker": "```plantuml",
  "markdown-pdf.plantumlCloseMarker": "```",
  "markdown-pdf.plantumlServer": "https://mode.fh-joanneum.at/-/plantuml",
  "cSpell.language": "en,de-AT",
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "microcontrollers"
  ],
  "editor.renderWhitespace": "all",
  "window.zoomLevel": 1.3,
  "cSpell.enableFiletypes": [
    "typst"
  ],
  "go.toolsManagement.autoUpdate": true,
}
/*
Extenstions:
autopep8: https://marketplace.visualstudio.com/items?itemName=ms-python.autopep8
