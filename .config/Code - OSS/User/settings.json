{
  "editor.tabSize": 2,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.associations": {
    "*.html": "html"
  },
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,
  "keyboard.dispatch": "keyCode",
  "latex-workshop.message.update.show": false,
  "latex-workshop.latex.outputDir": "./tmp",
  "latex-workshop.latex.recipes": [
    {
      "name": "pdflatex fh-toolchain",
      "tools": ["create_tmp", "pdflatex", "biber", "pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "biber",
      "command": "biber",
      "args": ["./tmp/thesis.bcf"]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": ["-output-directory=tmp", "%DOC%"]
    },
    {
      "name": "create_tmp",
      "command": "mkdir",
      "args": ["-p", "./tmp"]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.outDir": "./tmp",
  "window.titleBarStyle": "custom",
  "editor.fontFamily": "'JetBrains Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "terminal.integrated.fontFamily": "\"Source Code Pro\",\"Meslo LG M DZ for Powerline\"",
  "terminal.integrated.fontSize": 16,
  "java.errors.incompleteClasspath.severity": "ignore",
  "todo-tree.highlights.customHighlight": {
    "TODO": {},
    "FIXME": {}
  },
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "green",
    "type": "none"
  },
  "todo-tree.regex.regex": "((//|#|%+?|<!--|;|/\\*|^)\\s*($TAGS)|^\\s*- \\[ \\])",
  "todo-tree.general.tags": ["TODO", "FIXME", "todo"],
  "[python]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true
  },
  "remotefs.remote": {
